class Solution {
    public int[] diStringMatch(String S) {
        int small = 0;
        int large = S.length();
        int[] result = new int[large+1];
        for (int i=0; i<S.length(); i++) {
            if (S.charAt(i)=='I')
                result[i] = small++;
            else if (S.charAt(i)=='D')
                result[i] = large--;
        }
        result[S.length()] = small;
        return result;
    }
}
