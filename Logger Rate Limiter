class Logger {
    HashMap<String, Integer> log;
    public Logger() {
        log = new HashMap<>();
    }
    
    public boolean shouldPrintMessage(int timestamp, String message) {
        if (!log.containsKey(message)) {
            log.put(message, timestamp);
            return true;
        }
        else {
            int ts = log.get(message);
            if (timestamp-ts>=10) {
                log.put(message, timestamp);
                return true;
            }
        }
        return false;
    }
}
