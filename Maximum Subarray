class Solution {
    public int maxSubArray(int[] nums) {
        int maxSum = Integer.MIN_VALUE;
        if (nums.length == 1) {
            maxSum = nums[0];
        }
        for (int i=0; i<nums.length; i++) {
            int currSum = 0;
            for (int j=i; j<nums.length; j++) {
                currSum += nums[j];
                if (currSum > maxSum) {
                    maxSum = currSum;
                }
            }
        }
        return maxSum;
    }
}
