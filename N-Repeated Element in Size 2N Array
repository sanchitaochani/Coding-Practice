class Solution {
    public int repeatedNTimes(int[] A) {
        HashMap<Integer, Integer> uniqueValues=new HashMap<>();
        for (int i=0; i<A.length; i++) {
            if (uniqueValues.containsKey(A[i]))
                return A[i];
            uniqueValues.put(A[i],i);
        }
        return 0;
    }
}
