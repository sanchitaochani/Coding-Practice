class Solution {
    public String removeVowels(String S) {
        StringBuffer sb = new StringBuffer();
        for (int i=0; i<S.length(); i++) {
            if (S.charAt(i) != 'a' && 
               S.charAt(i) != 'e' &&
               S.charAt(i) != 'i' &&
               S.charAt(i) != 'o' &&
               S.charAt(i) != 'u' ) {
                sb.append(S.charAt(i));
            }
        }
        
        return sb.toString();
    }
}
