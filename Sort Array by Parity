class Solution {
    public int[] sortArrayByParity(int[] A) {
        int[] result = new int[A.length];
        int k=0;
        for (int i=0; i<A.length; i++) {
            if (A[i]%2 == 0)
                result[k++] = A[i];
        }
        for (int i=0; i<A.length; i++) {
            if (A[i]%2 != 0)
                result[k++] = A[i];
        }
        return result;
    }
}
