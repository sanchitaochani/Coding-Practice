class Solution {
    public int[] sortArrayByParityII(int[] A) {
        int[] result = new int[A.length];
        for (int i=0, j=0; i<A.length; i++) {
            if (A[i]%2==0) {
                System.out.println(A[i]);
                result[j] = A[i];
                j=j+2;
            }
        }
        for (int i=0, j=1; i<A.length; i++) {
            if (A[i]%2==1) {
                result[j] = A[i];
                j=j+2;
            }
        }
        return result;
    }
}
