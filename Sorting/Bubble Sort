private static void bubbleSort(int[] arr) {
		for (int i=0; i<arr.length; i++) {
			for (int j=0; j<arr.length; j++) {
				if (arr[i]<arr[j]) {
					int temp=arr[j];
					arr[j]=arr[i];
					arr[i]=temp;
				}
			}
		}
		System.out.println("Bubble sort");
		for (int i=0; i<arr.length; i++) {
			System.out.print(arr[i]+" ");
		}
	}
