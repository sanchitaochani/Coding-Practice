class Solution {
    public String toLowerCase(String str) { 
        for (int i=0; i<str.length(); i++) {
            int num = (int) str.charAt(i);
            if (num>=65 && num<=90) {
                num=num+32;
                str = str.replace(str.charAt(i), (char) num);
            }
        }
        return str;
    }
}
